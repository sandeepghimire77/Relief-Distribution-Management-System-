    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <h1>
                Dashboard
                <small>Control panel</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="/Dashboard"><i class="fa fa-dashboard"></i> Home</a></li>
                <li class="active">Dashboard</li>
            </ol>
        </section>

        <!-- Main content -->
        <section class="content">
            <!-- Small boxes (Stat box) -->
            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <h3>150</h3>
                            <p>Affected Places</p>
                        </div>
                        <div class="icon">
                            <i class="ion-ios-home-outline"></i>
                        </div>
                        <a href="/place/affectedarea" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div><!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">
                            <h3>53<sup style="font-size: 20px">%</sup></h3>
                            <p>Services</p>
                        </div>
                        <div class="icon">
                            <i class="ion-paper-airplane"></i>
                        </div>
                        <a href="/helptype/servicelist" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!--Add New Users-->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <h3>44</h3>
                            <p>Add New User</p>
                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                        <a href="/users/register" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!--User List-->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-red">
                        <div class="inner">
                            <h3>65</h3>
                            <p>Users</p>
                        </div>
                        <div class="icon">
                            <i class="ion-ios-people"></i>
                        </div>
                        <a href="/users/userlist" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
            </div>
            <!-- Main row -->
            <div class="row">
                <div class="form-table">
                    <form action="/place/Editaffectedarea" method="post">
                        {{#each rows}}
                            <div class="form-group row">

                                <label for="example-text-input" class="col-sm-2 col-form-label">Id</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.Id}} name="Id" id="example-text-input">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">Place Name</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.PlaceName}} name="placename">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">Zone Name</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.ZoneName}} name="zonearea">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">District Name</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.DistrictName}} name="districtname">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">Posted by Name</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.PostedByName}} name="postedBy">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">Detail Description</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.DetailDescription}} name="detaildescription">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">Contact</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value={{this.PhoneNumber}} name="contactnumber">
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="example-text-input" class="col-sm-2 col-form-label">Email</label>
                                <div class="col-sm-10">
                                    <input class="form-control" type="text" value="{{this.Email}}" name="email">
                                </div>
                            </div>
                            <input type="submit" name="submit" value="Save">
                        {{/each}}
                    </form>
                </div>
            </div><!-- /.row (main row) -->
        </section><!-- /.content -->
